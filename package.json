{
  "name": "starter-kit-li-element",
  "version": "1.1.0",
  "description": "component starter kit litelement",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./webpack.config.js --env.mode development",
    "prod": "webpack-dev-server --config ./webpack.config.js --env.mode production",
    "build": "webpack --config=webpack.config.js --env.mode production",
    "storybook": "start-storybook --node-resolve --watch --open",
    "build-storybook": "build-storybook",
    "test": "karma start --coverage",
    "test:ci": "karma start --browsers=ChromeHeadlessNoSandbox",
    "test:watch": "karma start --auto-watch=true --clesingle-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:bs": "karma start karma.bs.config.js --coverage",
    "lint:eslint": "eslint --ext .js,.html . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path .gitignore",
    "storybook:build": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/duplexAscensores/starter-kit-lit-element.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        120
      ],
      "type-enum": [
        2,
        "always",
        [
          "breaking",
          "feat",
          "fix",
          "refactor",
          "config",
          "test",
          "docs",
          "no-release"
        ]
      ]
    }
  },
  "author": "DanielBonus",
  "license": "ISC",
  "homepage": "https://gitlab.com/duplexAscensores/starter-kit-lit-element/-/blob/master/README.md",
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "core-js": "3",
    "lit-element": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@open-wc/demoing-storybook": "^1.13.6",
    "@open-wc/eslint-config": "^2.0.4",
    "@open-wc/testing": "^2.5.8",
    "@open-wc/testing-karma": "^3.3.8",
    "@open-wc/testing-karma-bs": "^1.3.52",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.2",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@webpack-cli/serve": "^0.2.0",
    "babel-loader": "^8.0.6",
    "babel-preset-minify": "^0.5.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "deepmerge": "^4.2.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "mini-css-extract-plugin": "^0.9.0",
    "portfinder-sync": "0.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config"
    ]
  }
}
